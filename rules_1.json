// it turns out blocking these will slow down youtube and make it impossible to play video
// YT is so evil
[
  {
    "id": 1,
    "priority": 1,
    "action": {
      "type": "block"
    },
    "condition": {
      "urlFilter": "https://www.youtube.com/*guide?*",
      "resourceTypes": ["xmlhttprequest", "script"],
      "initiatorDomains": ["www.youtube.com"]
    }
  },
  {
    "id": 2,
    "priority": 1,
    "action": {
      "type": "block"
    },
    "condition": {
      "urlFilter": "https://googleads.g.doubleclick.net/*pagead/id*",
      "resourceTypes": ["xmlhttprequest", "script"],
      "initiatorDomains": ["www.youtube.com"]
    }
  },
  {
    "id": 3,
    "priority": 1,
    "action": {
      "type": "block"
    },
    "condition": {
      "urlFilter": "https://*googlevideo.com*",
      "resourceTypes": ["xmlhttprequest", "script"],
      "initiatorDomains": ["www.youtube.com"]
    }
  },
  {
    "id": 4,
    "priority": 1,
    "action": {
      "type": "block"
    },
    "condition": {
      "urlFilter": "https://*.googlevideo.com/videoplayback*",
      "resourceTypes": ["media"]
    }
  }
]
